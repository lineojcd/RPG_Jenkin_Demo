pipeline {
    agent any
    stages {
        stage('build') {
            steps {
                echo  "first demo in python"
            }
        }
        stage('Test') {
            steps {
                echo 'Testing my first demo..'
            }
        }
        stage('Deploy') {
            steps {
                echo 'Deploying my first demo....'
            }
        }
    }
}
